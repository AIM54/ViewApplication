<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="InterestingTextView">
        <attr name="text" format="string|reference" />
        <attr name="textSize" format="dimension|reference" />
        <attr name="textColor" format="color|reference" />
        <attr name="duration" format="integer|reference" />
    </declare-styleable>
    <declare-styleable name="NewGuildeView">
        <attr name="guildBackgoundColor" format="color|reference" />
    </declare-styleable>
    <declare-styleable name="VerticalScrollView">
        <attr name="android:layout_gravity" />
        <attr name="layout_position">
            <enum name="middle" value="0" />
            <enum name="left" value="1" />
            <enum name="right" value="2" />
        </attr>
    </declare-styleable>
</resources>