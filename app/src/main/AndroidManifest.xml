<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bian.viewapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.ListViewActivity" />
        <activity android:name=".activity.RecyclerViewActivity" />
        <activity android:name=".activity.MaterialDesignActivity" />
        <activity android:name=".activity.NewListActivity" />
        <activity android:name=".activity.ViewGroupActivity" />
        <activity android:name=".activity.RefreshListActivity"></activity>
    </application>

</manifest>